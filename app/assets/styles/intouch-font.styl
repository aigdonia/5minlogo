@charset "UTF-8";

@font-face
  font-family: "intouch";
  src:url("../fonts/intouch.eot");
  src:url("../fonts/intouch.eot?#iefix") format("embedded-opentype"),
    url("../fonts/intouch.woff") format("woff"),
    url("../fonts/intouch.ttf") format("truetype"),
    url("../fonts/intouch.svg#intouch") format("svg");
  font-weight: normal;
  font-style: normal;

[data-icon]:before
  font-family: "intouch" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;

[class^="icon-"]:before,
[class*=" icon-"]:before
  font-family: "intouch" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
